  "scripts": {
    "start": "cross-env NODE_ENV=production node server/server.js",
    "build:client": "cross-env NODE_ENV=production webpack --config ./webpack/client/webpackProdConfig.js",
    "build:server": "cross-env NODE_ENV=development webpack --config ./webpack/server/webpackProdConfig.js",
    "dev:client": "cross-env NODE_ENV=development webpack-dev-server --config ./webpack/client/webpackDevConfig.js",
    "dev:server": "nodemon ./server/server.js",
    "clear": "rimraf dist",
    "lint": "eslint .",
    "fix": "eslint --fix .",
    "dev": "npm-run-all -p -l fix dev:*",
    "build": "npm-run-all -s -l clear build:client build:server"
